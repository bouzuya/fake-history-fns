{
  "name": "fake-history-fns",
  "description": "fake-history functions",
  "version": "0.1.3",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "8"
          }
        }
      ],
      "power-assert"
    ]
  },
  "beater": {
    "dir": "./.tmp/es5/test/",
    "require": [
      "./scripts/enable-power-assert.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/bouzuya/fake-history-fns/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@types/node": "7.0.12",
    "@types/power-assert": "1.4.29",
    "babel-preset-power-assert": "^3.0.0",
    "beater": "0.4.1",
    "beater-cli": "0.2.4",
    "cpx": "1.5.0",
    "npm-run-all": "4.0.2",
    "power-assert": "1.4.2",
    "proxyquire": "1.7.11",
    "rimraf": "2.6.1",
    "tslint": "5.0.0",
    "typescript": "2.2.2",
    "watch": "1.0.2"
  },
  "files": [
    "_",
    "back.d.ts",
    "back.js",
    "create.d.ts",
    "create.js",
    "current-state.d.ts",
    "current-state.js",
    "current.d.ts",
    "current.js",
    "forward.d.ts",
    "forward.js",
    "go.d.ts",
    "go.js",
    "index.d.ts",
    "index.js",
    "length.d.ts",
    "length.js",
    "next-state.d.ts",
    "next-state.js",
    "next.d.ts",
    "next.js",
    "previous-state.d.ts",
    "previous-state.js",
    "previous.d.ts",
    "previous.js",
    "push-state.d.ts",
    "push-state.js",
    "replace-state.d.ts",
    "replace-state.js",
    "state.d.ts",
    "state.js",
    "types.d.ts",
    "types.js"
  ],
  "homepage": "https://github.com/bouzuya/fake-history-fns",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/bouzuya/fake-history-fns.git"
  },
  "scripts": {
    "build": "npm-run-all -s 'build:es2015' 'build:es5' 'build:lib'",
    "build:es2015": "tsc",
    "build:es5": "babel --out-dir .tmp/es5 --source-maps inline .tmp/es2015",
    "build:lib": "babel --copy-files --no-comments --out-dir ./lib .tmp/es2015/src",
    "clean": "rimraf *.js *.d.ts _ .tmp public",
    "lint": "tslint --type-check --project tsconfig.json",
    "prepublish": "npm-run-all -s 'clean' 'lint' 'build'",
    "start": "node .tmp/es2015/src/server/index.js",
    "test": "beater",
    "watch": "npm-run-all -p 'watch:*'",
    "watch:es2015": "npm run 'build:es2015' -- --watch",
    "watch:test": "watch --wait 2 'npm test' '.tmp/es5/'",
    "watch:ts": "npm run 'build:ts' -- --sourceMap --watch"
  }
}
