{
  "name": "fake-history-fns",
  "description": "fake-history functions",
  "version": "0.1.3",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "beater": {
    "dir": "./.tmp/es5/test/",
    "require": [
      "./scripts/enable-power-assert.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/bouzuya/fake-history-fns/issues"
  },
  "devDependencies": {
    "@types/node": "7.0.12",
    "@types/power-assert": "1.4.29",
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-preset-es2015": "6.24.0",
    "beater": "0.4.1",
    "beater-cli": "0.2.4",
    "cpx": "1.5.0",
    "espower-loader": "1.2.0",
    "npm-run-all": "4.0.2",
    "power-assert": "1.4.2",
    "proxyquire": "1.7.11",
    "rimraf": "2.6.1",
    "tslint": "5.0.0",
    "typescript": "2.2.2",
    "watch": "1.0.2"
  },
  "files": [
    "_",
    "back.d.ts",
    "back.js",
    "create.d.ts",
    "create.js",
    "current-state.d.ts",
    "current-state.js",
    "current.d.ts",
    "current.js",
    "forward.d.ts",
    "forward.js",
    "go.d.ts",
    "go.js",
    "index.d.ts",
    "index.js",
    "length.d.ts",
    "length.js",
    "next-state.d.ts",
    "next-state.js",
    "next.d.ts",
    "next.js",
    "previous-state.d.ts",
    "previous-state.js",
    "previous.d.ts",
    "previous.js",
    "push-state.d.ts",
    "push-state.js",
    "replace-state.d.ts",
    "replace-state.js",
    "state.d.ts",
    "state.js",
    "types.d.ts",
    "types.js"
  ],
  "homepage": "https://github.com/bouzuya/fake-history-fns",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/bouzuya/fake-history-fns.git"
  },
  "scripts": {
    "build": "npm-run-all -s 'build:ts' 'build:es2015' 'build:lib'",
    "build:es2015": "babel .tmp/es2015/ -d .tmp/es5/",
    "build:lib": "npm-run-all -p 'build:lib:*'",
    "build:lib:dts": "cpx '.tmp/es2015/src/**/*.d.ts' .",
    "build:lib:js": "cpx '.tmp/es5/src/**/*.js' .",
    "build:ts": "tsc",
    "clean": "rimraf *.js *.d.ts _ .tmp public",
    "lint": "tslint --type-check --project tsconfig.json",
    "prepublish": "npm-run-all -s 'clean' 'lint' 'build'",
    "start": "node .tmp/es2015/src/server/index.js",
    "test": "beater",
    "watch": "npm-run-all -p 'watch:*'",
    "watch:es2015": "npm run 'build:es2015' -- --watch",
    "watch:test": "watch --wait 2 'npm test' '.tmp/es5/'",
    "watch:ts": "npm run 'build:ts' -- --sourceMap --watch"
  }
}
