{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "ordered-imports": [
      true,
      {
        "named-imports-order": "lowercase-last"
      }
    ],
    "quotemark": [
      true,
      "single"
    ],
    "strict-boolean-expressions": true,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  },
  "rulesDirectory": []
}
